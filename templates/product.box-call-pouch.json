            "accordion_row_XE9tYL": {
              "type": "_accordion-row",
              "settings": {
                "heading": "How is this different from a standard barricade bag?",
                "open_by_default": false,
                "icon": "none",
                "width": 20
              },
              "blocks": {
                "text_GYYtLC": {
                  "type": "text",
                  "settings": {
                    "text": "<p>The Delta Zero uses a three-chamber design with independent fill zones and wrap-around compression, creating multiple contact points and better stability than a single-chamber cube/wedge.</p>",
                    "width": "100%",
                    "max_width": "normal",
                    "alignment": "left",
                    "type_preset": "rte",
